{
  "name": "ngbp-spell-webapp",
  "version": "0.0.1",
  "description": "Lint javascript files",
  "homepage": "https://github.com/ngbp/spell-webapp",
  "author": {
    "name": "NGBP Team",
    "url": "http://ngbp.org"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ngbp/spell-webapp.git"
  },
  "bugs": {
    "url": "https://github.com/ngbp/spell-webapp"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ngbp/spell-webapp/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "peerDependencies": {
    "ngbp": "~0.4"
  },
  "dependencies": {
    "jshint-stylish": "~0.1.5",
    "gulp-jshint": "~1.3.4"
  },
  "keywords": [
    "ngbpplugin",
    "ngbpspell"
  ],
  "ngbp": {
    "default": [ "webapp-build" ],
    "paths": {
      "source": "build",
      "source_assets": "<%= paths.source %>/assets"
    },
    "globs": {
      "source": {
        "js": [
          "<%= paths.source %>/**/*.js",
          "!<%= paths.source %>/**/*.spec.js",
          "!<%= paths.source_assets %>/**/*"
        ]
      }
    },
    "tasks": {
      "webapp-lintjs": {
        "lint": {
          "bitwise": true,
          "curly": true
        },
        "reporter": "jshint-stylish"
      }
    }
  }
}

